	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"Object.prototype.realizarLogin = function (pm) {",
					"    _sendRequestToken(pm, ",
					"        pm.environment.get(\"email_paciente\"), ",
					"        pm.environment.get(\"senha\"),",
					"        \"paciente\");",
					"}",
					"",
					"Object.prototype.logarComCredenciais = function (pm, perfil ) {",
					"    email = \"email_\" + perfil;",
					"    _sendRequestToken(pm, ",
					"        pm.environment.get(email), ",
					"        pm.environment.get(\"senha\"),",
					"        perfil);",
					"}",
					"",
					"// Função para gerar token",
					"function _sendRequestToken(pm, username, password, perfil) {",
					"    var url = pm.environment.get(\"base_url\") + \"/fiap-hmv/v1/login\";",
					"    ",
					"    const body = {",
					"        mode: 'raw',",
					"        raw: JSON.stringify(",
					"            {",
					"                email: username,",
					"                password: password,",
					"                device_name: \"fiap_hmv_mobile\"",
					"            }",
					"        )",
					"    }",
					"",
					"    pm.sendRequest({",
					"        url: url,",
					"        method: 'POST',",
					"        header: {",
					"            'Content-Type': 'application/json',",
					"        },",
					"        body: body",
					"    }, function (err, response) {",
					"        if (response.code == 200) {",
					"            var token = response.text();",
					"            console.log(\"token \" + token)",
					"            pm.collectionVariables.set(\"token_\" + perfil, token);",
					"        } else {",
					"            console.error(response, \"A requisição para gerar token FALHOU\");",
					"            // pm.expect.fail(`Geração de token falhou. Status code: ${response.code}`);",
					"        }",
					"    });",
					"}",
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"//validar retorno do serviço de acordo com a variável {{expected_status_code}}",
					"pm.test(\"Validação do status code\", function () {",
					"    console.log(\"Validação do status code direto da coleção\");",
					"    pm.response.to.have.status(pm.collectionVariables.get(\"status_code_esperado\"));",
					"});",
					"",
					"Object.prototype.validarBodyVazio = function (pm) {",
					"",
					"    pm.test(\"Validação do body vazio\", function () {",
					"        pm.response.to.have.body(\"\");",
					"    });",
					"}",
					"",
					"Object.prototype.validarBodyPreenchido = function (pm) {",
					"",
					"    pm.test(\"Validação do body preenchido\", function () {",
					"        pm.response.to.not.have.body(\"\");",
					"    });",
					"}",
					"",
					"// O campo local deve conter todo o caminho até chegar no valor do JSON",
					"// Para informar mais de um item da lista, separe as opções com ;",
					"Object.prototype.validateResponseBodyFieldValue = function (pm, local, expectedValue) {",
					"",
					"    pm.test(\"Validação do campo/objeto/array \" + local, function () {",
					"",
					"        var jsonData = JSON.parse(JSON.stringify(pm.response.json()));",
					"        var valueFromResponse = extractValueFromField(jsonData, local);",
					"        console.log('expect ' + expectedValue)",
					"        if (expectedValue == \"\" || expectedValue == null || expectedValue == undefined || expectedValue == []) {",
					"            pm.expect(valueFromResponse).to.eql(expectedValue);",
					"        } else {",
					"            var lstExpectedValue = expectedValue.split(';'); // Transformar em lista os possíveis retornos",
					"",
					"            // validar a qtd de itens do array do request(lstExpectedValue) e reponse(valueFromResponse)",
					"            if (Array.isArray(valueFromResponse)) {",
					"                pm.expect(lstExpectedValue.length, \"Validação da qtd de itens da lista\").to.eql(valueFromResponse.length);",
					"            }",
					"            // validar contains cada item",
					"            lstExpectedValue.forEach(function (value, c) {",
					"                pm.expect(valueFromResponse, \"Validação de cada item da lista\").to.contains(value);",
					"            });",
					"        }",
					"",
					"    });",
					"}",
					"",
					"",
					"// Extrair os dados de campos especificos e para tal informar os campos separados por ponto final",
					"// Exemplo: details.pathParameters.organizationId",
					"function extractValueFromField(obj, path) {",
					"    return path.split('.').reduce(function (prev, curr) {",
					"        return prev ? prev[curr] : null",
					"    }, obj || self)",
					"}",
					"",
					"Object.prototype.validateDefaultResponseBody = function (pm, message, status, data) {",
					"",
					"    var jsonData = JSON.parse(JSON.stringify(pm.response.json()));",
					"",
					"    pm.test(\"Validação da resposta de erro\", function () {",
					"        pm.expect(message, \"Validação do campo mensagem\").to.eql(jsonData.mensagem);",
					"        pm.expect(status, \"Validação do campo status\").to.eql(jsonData.status);",
					"        pm.expect(data, \"Validação do campo detalhes\").to.eql(jsonData.detalhes);",
					"    });",
					"}",
					"",
					"Object.prototype.validateBody = function (pm, expectedValue) {",
					"    pm.test(\"Validação do body\", function () {",
					"        pm.response.to.have.body(expectedValue);",
					"    });",
					"}"
				]
			}
		}
